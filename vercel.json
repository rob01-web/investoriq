{
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60
    }
  },
  "redirects": [
    { "source": "/app", "destination": "/dashboard", "permanent": false }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
